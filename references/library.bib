@article{larssonWinterOptimalLogging2014,
  title = {Winter - the Optimal Logging Season to Sustain Growth and Performance of Retained Epiphytic Lichens in Boreal Forests},
  author = {Larsson, Per and Solhaug, Knut Asbjorn and Gauslaa, Yngvar},
  year = {2014},
  month = dec,
  volume = {180},
  pages = {108--114},
  issn = {0006-3207},
  doi = {10.1016/j.biocon.2014.09.045},
  abstract = {Epiphytic lichens, comprising an important biodiversity component with significant functions in old boreal forests, are susceptible to logging. Leaving retention trees may partly compensate for the adverse effects of logging, but the impact of logging season on retained lichens is unknown. To identify the least harmful logging season seen from an epiphytic lichen perspective, we simulated logging events by transplanting two old forest model species - Lobaria pulmonaria (cephalolichen) and Lobaria scrobiculata (cyanolichen) - in summer, autumn, winter and spring, respectively. Lichens were collected in intact old forests and transplanted in clear-cuts. They were placed on five aspects (N, E, S, W, and top) on artificial stands covered with bark. They all gained biomass and area in the following year. However, logging in January resulted in subsequent higher relative growth rates than in lichens being exposed to clear cuts in other seasons, consistent with clear seasonal logging effects. Aspect impacted lichen growth less than logging season, despite significant bleaching and chlorophyll contents in the most exposed aspects. The high annual growth rate and the low level of damage after transplantation to clear cuts during winter suggest that green tree retention (GTR) logging practices in winter can support life-boating of susceptible old forest lichen epiphytes. Logging simulation during other seasons than winter resulted in lower growth, implying higher risk of permanent damage. Successful use of GTR cannot compensate for the loss of epiphytes at timber harvest, but supports local retained lichen populations that subsequently may boost re-colonization of the young stand. (C) 2014 Elsevier Ltd. All rights reserved.},
  journal = {BIOLOGICAL CONSERVATION}
}

@article{Latifi,
  title = {Estimating Over-and Understorey Canopy Density of Temperate Mixed Stands by Airborne {{LiDAR}} Data},
  author = {Latifi, Hooman and Heurich, Marco and Hartig, Florian and Rg, J{\"o} and Ller, M{\"u} and Krzystek, Peter and Jehl, Hans and Dech, Stefan},
  doi = {10.1093/forestry/cpv032},
  abstract = {Estimating forest structural attributes is one of the essential forestry-related remote sensing applications. The methods applied so far typically concentrate on the structure of the overstorey. For various conservation and man-agement applications, however, information on lower layers is also of great interest. Detecting understorey cover by remote sensing is challenging, as passive sensors do not penetrate to the forest ground layer. An alternative to these is 3D metrics from active light detection and ranging (LiDAR). Here, we evaluate this technique for describing the vegetation density of multiple stand layers within the temperate stands of a large protected area in south-eastern Germany. We combined LiDAR metrics and information on forest habitat types with regression models to investigate LiDAR metrics that are significantly correlated with vegetation density. The top canopy and the herbal layer showed strong correlations with the applied LiDAR metrics, whereas the predictive power was lower for the intermediate stand layers. Moreover, our results suggest that the relationship between LiDAR predictors and vegetation density depends on the forest type. A comparison of the regression models with random forest pre-dictions showed no major improvement in predictive error. In conclusion, this study highlights the value of the LiDAR metrics for characterizing the structural properties of lower forest layers, which has implications for wildlife and forest management applications, especially in protected areas.},
  file = {/Volumes/GoogleDrive/My Drive/PhD/zotero/library/Latifi et al Estimating over-and understorey canopy density of temperate mixed stands by airborne LiDAR data.pdf}
}

