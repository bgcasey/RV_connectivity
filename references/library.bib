@article{bartelsBryophyteAbundanceDiversity2018,
  title = {Bryophyte Abundance, Diversity and Composition after Retention Harvest in Boreal Mixedwood Forest},
  author = {Bartels, Samuel F and Macdonald, S Ellen and Johnson, Derek and Caners, Richard T and Spence, John R},
  year = {2018},
  month = mar,
