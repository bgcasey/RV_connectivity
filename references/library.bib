@article{LeBlanc2010,
  title = {Short Term Response of Small Mammals and Forest Birds to Silvicultural Practices Differing in Tree Retention in Irregular Boreal Forests},
  author = {Le Blanc, M{\'e}lanie-Louise and Fortin, Daniel and Darveau, Marcel and Ruel, Jean-Claude},
  year = {2010},
  month = sep,
  volume = {17},
