@article{Bellamy2009,
  title = {Willow {{Warbler Phylloscopus}} Trochilus Habitat in Woods with Different Structure and Management in Southern {{England}}},
  author = {Bellamy, Paul E. and Hill, Ross A. and Rothery, Peter and Hinsley, Shelley A. and Fuller, Robert J. and Broughton, Richard K.},
