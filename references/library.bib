  author = {He, Kate S. and Bradley, Bethany A. and Cord, Anna F. and Rocchini, Duccio and Tuanmu, Mao-Ning Ning and Schmidtlein, Sebastian and Turner, Woody and Wegmann, Martin and Pettorelli, Nathalie},
