@techreport{MacLean2010,
  title = {Forest {{Dynamics}}, {{Succession}} and {{Habitat Relationships Under Differing Levels}} of {{Silviculture}}},
  author = {MacLean, D A and Adams, Greg and Pelletier, Gaetan and {Amos-Binks}, Luke and Carle, J.-F. and Chicoine, Bruno and {Colford-Gilks}, Amanda and Forget, Pascale and Hach{\'e}, Samuel and Henderson, Julie and La France, K and Poulin, J.-F. and Smith, Matthew and Ward, Chris and Witkowski, Amy and Beckley, Tom M and B{\'e}land, Martin and Betts, Matthew G and Erdle, Thom A and Forbes, Graham J and Frego, Kate and Kershaw Jr., J A and Roberts, Mark R and Roy, Roger and Samson, Claude and Villard, Marc-Andr{\'e} and Wagner, Robert G and Wilson, Jeremy A},
  year = {2010},
  pages = {75 pp.},
  institution = {{Sustainable Forest Management Network}},
  abstract = {Understanding forest dynamics, succession, and habitat relationships under differing levels of silviculture is the basis of forest management planning. The Acadian forest is comprised of a complex mix of forest communities, including mixedwood stands where boreal softwood and southern hardwoods commingle. This forest is ecologically unique in North America and economically important in northeast North America. The forest provides many ecological services, is biologically diverse, and is the raw material source for the forest industry \textendash{} a mainstay of the Maritime economy. The succession dynamics of this ecosystem are strongly influenced by natural disturbances, and by a long history of harvest and protection from fire and insects. This report summarizes the findings of 12 graduate student studies that were funded through the Sustainable Forest Management Network (SFMN) and resulted in the publication of several scholarly articles. The overall goals of the project were to: Better understand the succession dynamics of the Acadian mixedwood forest, its value as habitat, and the diversity and habitat implications of current management, and Use this understanding to develop and evaluate alternative stand and forest management strategies aimed at maintaining the diversity and habitat values of the forest while supplying an economic supply of industrial raw material.},
  file = {/Volumes/GoogleDrive/My Drive/PhD/zotero/library/MacLean et al (2010) Forest Dynamics, Succession and Habitat Relationships Under Differing Levels of Silviculture.pdf},
  isbn = {9781552612651}
}

@article{Mahon2016,
  title = {Community Structure and Niche Characteristics of Upland and Lowland Western Boreal Birds at Multiple Spatial Scales},
  author = {Mahon, C. Lisa and Holloway, Gillian and S{\'o}lymos, P{\'e}ter and Cumming, Steve G. and Bayne, Erin M. and Schmiegelow, Fiona K.A. and Song, Samantha J.},
  year = {2016},
  month = feb,
  volume = {361},
  pages = {99--116},
  publisher = {{Elsevier B.V.}},
  issn = {03781127},
  doi = {10.1016/j.foreco.2015.11.007},
  abstract = {Direct and indirect effects of disturbance may cause the decline of specialist species and alter the condition of ecological communities. We characterized the community structure and niche characteristics (niche position, marginality, breadth) of upland and lowland boreal birds at scales relevant to both natural and human disturbance patterns in western boreal forests undergoing rapid and extensive multi-sector resource development. Our goal was to identify the degree of ecological specialization in order to inform activities directed at conserving a diversity of species (e.g. specialists and generalists) within the western boreal bird community. We used avian data ({$>$}5,220 point counts) and environmental variable data comprised of forest composition, stand, and landscape pattern metrics at local (7.1. ha), landscape (1,963. ha), and regional (11,310. ha) scales to determine boreal bird distribution and community-level associations using Canonical Correspondence Analysis (CCA) and Outlying Mean Index (OMI) analysis. OMI analysis explained a high proportion of variance in the dataset (71.8\%) and separated boreal birds along two axes associated with moisture-productivity and age-structural complexity gradients. Niche position was influenced by local scale variables (height, age, area of mature-old forest, area of wet soil types), but also landscape and regional scale variables (total area of hardwood and conifer, mean nearest neighbour distance of conifer, and total core area of productive upland conifer). Only 15 of 67 species (22\%) had marginal (atypical) niches and narrow niche breadths exhibiting specialization in old hardwood and white spruce forests and burned, open, and lowland habitats. Most species occupied typical or common habitats within the study area and exhibited generalist strategies typical of species in heterogeneous and disturbed habitats that undergo frequent change. Our results suggest the need to design and implement multi-species plans to conserve a diversity of western boreal bird species (e.g. specialists and generalists) at the regional scale.},
  file = {/Volumes/GoogleDrive/My Drive/PhD/zotero/library/Mahon et al (2016) Community structure and niche characteristics of upland and lowland western boreal birds at multiple spatial scales.pdf},
  isbn = {0378-1127},
  journal = {Forest Ecology and Management},
  keywords = {Boreal birds,Community structure,Niche characteristics,Niche position,Oil sands areas,Outlying Mean Index}
}

